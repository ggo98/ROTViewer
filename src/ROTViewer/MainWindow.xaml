<Window x:Class="ROTViewer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Running Object Table Viewer" Height="398" Width="576" Loaded="Window_Loaded" Icon="/ROTViewer;component/Images/List%2048%20n%20p.png">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="115*"/>
            <ColumnDefinition Width="77*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button  Content="Refresh Table"  Name="refreshTable" Click="refreshTable_Click" Grid.ColumnSpan="2" />
        <ListBox  Grid.Row="1" Name="monikerList" ItemsSource="{Binding Path=Monikers }" Grid.ColumnSpan="2" FontSize="16" FontFamily="Cascadia Code" />
    </Grid>
</Window>
